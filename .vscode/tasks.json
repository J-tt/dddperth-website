{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "command": "yarn",
      "problemMatcher": "$tsc",
      "args": ["tsc"]
    },
    {
      "type": "npm",
      "script": "build",
      "group": "build"
    },
    {
      "label": "Start dev server",
      "type": "npm",
      "script": "dev",
      "problemMatcher": []
    }
  ]
}
